@{
    ViewData["Title"] = "注冊";
}
<h2>@ViewBag.MErr</h2>
<form method="post" action="~/Manager/Rgistered">
    <div class="form-group row">
        <label for="inputUserName" class="col-sm-2">帳號名稱</label>
        <div class="col-sm-10">
            <input type="text" id="inputUserName" name="Uname" class="form-control" />
        </div>
        <br />
    </div>
    <div class="form-group row">
        <label for="inputPasswd" class="col-sm-2">帳號密碼</label>
        <div class="col-sm-10">
            <input type="text" placeholder=" 請輸入賬號（開頭為字母,且加數字至少6位）" required="required" id="inputPasswd" name="Userid" class="form-control" />
        </div>
        <br />
    </div>
    <div class="form-group row">
        <label for="inputName" class="col-sm-2">帳號姓名</label>
        <div class="col-sm-10">
            <input type="text" placeholder=" 請輸入密碼（開頭為字母,且包含數字共至少5位）" required="required" id="inputName" name="Passwd" class="form-control" />
        </div>
        <br />
    </div>
    <input class="btn btn-info" type="submit" value="新增" />
</form>