@{
    ViewData["Title"] = "注冊";
}
<p>@ViewBag.Err</p>
<form method="post" action="~/Home/Rgistered">
    <div class="form-group row">
        <label for="inputUserName" class="col-sm-2">帳號名稱</label>
        <div class="col-sm-10">
            <input type="text" id="inputUserName" name="UName" class="form-control" />
        </div>
    </div>
    <div class="form-group row">
        <label for="inputPasswd" class="col-sm-2">賬號</label>
        <div class="col-sm-10">
            <input type="text" placeholder=" 請輸入賬號" required="required" id="inputPasswd" name="UserId" class="form-control" />
        </div>
    </div>
    <div class="form-group row">
        <label for="inputName" class="col-sm-2">密碼</label>
        <div class="col-sm-10">
            <input type="text" placeholder=" 請輸入密碼" required="required" id="inputName" name="Passwd" class="form-control" />
        </div>
    </div>
    <input class="btn btn-info" type="submit" value="新增" />
</form>